var saldoDia = 0;

const consumoVeiculo = 9.0;

var litrosAbastecidos = 0;
var litrosGastos = 0;

document.write("saldoDay =<br>[");
for(var i = 0; i < spents.length; i++)
{
    litrosAbastecidos = supplies[i].value/prices[i].value;
    //document.write("debug:  Litros abastecidos em ", supplies[i].date, " ", litrosAbastecidos.toFixed(2), "<br>");
    litrosGastos = spents[i].value / consumoVeiculo;
    //document.write("debug: Litros gastos em ", spents[i].date, " ", litrosGastos.toFixed(2), "<br>");
    saldoDia = litrosAbastecidos - litrosGastos;
    if(saldoDia > 0)
        //document.write("debug: O saldo do veículo no dia ", spents[i].date, " é de ", saldoDia.toFixed(2), " Litros<br>")
        document.write("{<br>    date: '", spents[i].date,"',<br>    value: ", saldoDia.toFixed(2), "<br>},");
    /*else
        document.write("Na data em questão, não houve informação do valor da gasolina<br>");*/
}
document.write("]");

